@reboot sleep 20 && python ~/gateway/common.py # initialize confi settings in memory (new sys)
@reboot sleep 30 && python ~/gateway/gateway.py -ig # initialize gsm (new sys)
*/2 * * * * python ~/gateway/gateway.py -um smsoutbox # delete sent messages (new sys)

*/15 * * * * python ~/gateway/health.py # store health messages in memory (new sys)

* * * * * python ~/gateway/gateway.py -sm # send messages from memory (new sys)

@reboot sleep 60 && screen -S ring -d -m sudo python ~/gateway/ringservice.py # (new sys)
@reboot sleep 60 && screen -S rain -d -m sudo python ~/gateway/raindetect.py # (new sys)

@reboot sudo /opt/vc/bin/tvservice -o # (new sys)

